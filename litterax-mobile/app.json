{
  "expo": {
    "name": "Litterax",
    "description": "Multiplayer game based on words and power-ups. Compete against other players and unlock more minigames.",
    "slug": "litterax-mobile",
    "version": "0.0.1",
    "orientation": "portrait",
    "scheme": "litteraxmobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "platforms": ["ios", "android"],
    "githubUrl": "https://github.com/carlosvasquez25/litterax-project.git",
    "primaryColor": "#00C950",
    "icon": "./assets/images/icon.png",
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.anonymous.litteraxmobile"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#f0fafa",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.litteraxmobile"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
